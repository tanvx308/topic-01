package vn.com.fis.model;

public class Info {
	private int hp;
	private int s;
	private int d;
	private String t;
	
	public Info() {
	}

	public Info(int hp, int s, int d, String t) {
		this.hp = hp;
		this.s = s;
		this.d = d;
		this.t = t;
	}

	public int getHp() {
		return hp;
	}

	public void setHp(int hp) {
		this.hp = hp;
	}

	public int getS() {
		return s;
	}

	public void setS(int s) {
		this.s = s;
	}

	public int getD() {
		return d;
	}

	public void setD(int d) {
		this.d = d;
	}

	public String getT() {
		return t;
	}

	public void setT(String t) {
		this.t = t;
	}
	
	
}
