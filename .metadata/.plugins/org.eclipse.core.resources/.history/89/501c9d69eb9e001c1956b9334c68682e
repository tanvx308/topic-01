package vn.com.fis.model;

public class VehicleTicket {
	//properties
	private int code;
	private double price;
	
	//constructor
	public VehicleTicket() {
	}
	

	public VehicleTicket(int code, double price, double cost, double tax) {
		this.code = code;
		this.price = price;
	}


	//setter and getter
	public int getCode() {
		return code;
	}

	public void setCode(int code) {
		this.code = code;
	}

	public double getPrice() {
		return price;
	}

	public void setPrice(double price) {
		this.price = price;
	}


	@Override
	public String toString() {
		return "VehicleTicket [code=" + code + ", price=" + price + "]";
	}

	
}
